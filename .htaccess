RewriteEngine On
RewriteBase /msduran-nabua/

# Exclude assets from rewrite rule
RewriteCond %{REQUEST_URI} !\.(css|js|svg|jpg|jpeg|png|gif|woff2|ico|ttf)$ [NC]

# Redirect all requests to index.html
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /msduran-nabua/public/index.html [L]

# Exclude the public and app directories from the rewrite rule
RewriteCond %{REQUEST_URI} !^/msduran-nabua/public/
RewriteCond %{REQUEST_URI} !^/msduran-nabua/app/
RewriteRule ^(.*)$ /msduran-nabua/public/$1 [L]
